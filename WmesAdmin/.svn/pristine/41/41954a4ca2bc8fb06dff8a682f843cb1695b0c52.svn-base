/* CSS Document */
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, img, p, blockquote, th, td { margin: 0; padding: 0; }
body { font-size: 14px; color: #333; font-family: Verdana, Microsoft YaHei, Simsun; background: #FFFFFF; }
img { border: 0; vertical-align: middle; }
ol, ul, li { list-style: none; }
a { text-decoration: none !important; border: 0; outline: none; }
a:focus { outline: none; }
input, button, select, textarea { outline: none; }
.clear { clear: both; }

.logo { background: url(../images/logo.png) no-repeat left; height: 50px; display: block; border: 0 !important; }
.logo-name{margin-left: 124px; font-size: 24px; color: #fff; font-weight: bold;}
.logo2 { background: url(../images/logo.png) no-repeat 220px 0 #FFFFFF;margin-left:-285px; }
.border0 { border-radius: 0; margin-bottom: 0 !important; border: 0 !important; }
.name-bg { background: url(../images/bg-name.png) no-repeat 220px 0 #FFFFFF;margin-left:-220px; }
.pd0 { padding: 0 !important }
.pd10{ padding: 5px !important}
.pdt20{ padding-top:20px;}


.left-side { padding-bottom:50px;width: 200px;position: fixed;top: 50px;left: 0;overflow: hidden; background-color:#3d3d3d;height:1100px;}
.right-side { width: 100%; background: #FFF; height: 100%; float: left;}
.r-side-container { margin-left: 200px; padding:15px; }

.l-side-menu li{transition:all .2s ease;position: relative;}
.l-side-menu li a{min-height: 20px;padding: 16px 0;line-height: 20px;width:200px;display:block;color:#f1f1f1 !important;padding-left:15px;padding-right: 30px;overflow: hidden;}
.l-side-menu li:hover{ background-color: #333;}
.l-side-menu > li > a{border-top:1px solid #5c5c5c;}
.l-side-menu > li:nth-child(1) > a{border-top:1px solid transparent;}

.l-side-sub{background-color: #41444b;}
.l-side-sub li a{padding-left:38px;color:#bdbdbd !important;}
.l-side-sub li a:hover{ background-color: #333;color:#fff !important;}
.l-side-sub-crr > a{background-color: #575757 !important;}
.l-side-menu .l-side-sub-crr > a{color:#fff !important;}
.l-side-sub .l-sub-icon{background:url(../images/left-sub-down.png) center no-repeat;}
.l-side-subsub{background-color: #41444b;}
.l-side-subsub li a{padding-left:38px;}
.l-side-subsub li a:hover{ background-color: #333;}
.l-side-subsub-crr a{background-color: #FF9900 !important;}
.l-side-subbread{width:29px;height:29px;padding:8px 7px;background:#101010;cursor:pointer;margin:10px 10px 10px auto;}
.l-side-subbread span{display:block;width:100%;height:1px;background: #ababab;margin-top:3px;}
.l-side-subbread span:nth-child(1){margin-top:0;}
.l-side-subbread:hover span{background:#fff;}

.l-side-subhide{width:50px;overflow:visible;}
.l-side-subhide .l-sub-icon{display:none;}
.l-side-subhide .l-side-menu li > a{color:transparent !important;}
.l-side-subhide .l-side-menu li > a > img{margin-right:17px !important;}
.l-side-subhide .l-side-menu li{overflow:hidden;}

.l-side-subhidecontainer{margin-left:50px;}
.l-side-subhide .l-side-subsub{display:none !important;}
.l-sub-icon{background:url(../images/left-sub-down.png) center no-repeat;width: 18px;height: 10px;display:inline-block;transform:rotate(-90deg);transition:0.2s all ease;position: absolute;right: 3px;top: 22px;}
.l-side-subhide .l-side-menu > li:hover{overflow:visible;}
.l-side-subhide .l-side-menu > li:hover a{color:#fff !important;width:250px;background:#3d3d3d;}
.l-side-subhide .l-side-menu .l-side-sub> li a{color:#bdbdbd !important;width:200px;background:#3d3d3d;}
.l-side-subhide .l-side-menu > li > ul{position:absolute;left:50px;width:200px;} 
.l-side-subhide .l-side-menu > li:hover > ul{display:block !important;} 
.navbar-default .navbar-nav > li > a{color:#ddd !important;}
.navbar-default .navbar-nav > li > a:hover{background:#383838 !important;color:#fff !important;}
.l-side-subhide .l-side-sub > li:hover{overflow:visible;}
.l-side-subhide .l-side-sub > li:hover ul{display:block !important;position:absolute;left:200px;width:200px;top:0px;background:red;color:blue;}
.l-side-subhide .l-side-subsub li a,.l-side-subhide .l-side-sub > li a{background:#3d3d3d;color:#bdbdbd !important;}
.l-side-subhide .l-side-subsub li a:hover,.l-side-subhide .l-side-sub > li a:hover{background:#333 !important;color:#fff !important;}
.l-side-subhide .iScrollVerticalScrollbar{display:none !important;}
.l-side-menu > li a > img{margin-right:5px;}

/*取消bootstrap自带nav样式*/
.nav .open > a, .nav .open > a:hover, .nav .open > a:focus{background:transparent;}
.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus{background:transparent;}

.ico{transform:rotate(0deg); transition:0.2s all ease}


.row label{ font-weight:normal; padding:0 5px}

.btn-def{background-color: #2d80ce; width:100px; display:block; 
margin-left:150px; border:0 !important; transition:ease .2s background-color;}
.btn-def:hover{background-color: #66a5df;}


.btn-blue-s{background-color: #2d80ce; margin:10px 2px; padding:5px 20px;  text-align:center; color:#FFFFFF!important;; display:inline-block; border:0 !important; transition:ease .2s background-color; border-radius:4px}
.btn-blue-s:hover{background-color: #66a5df;}

.btn-blue-s2{background-color: #66a5df; margin:10px 2px; padding:5px 20px;  text-align:center; color:#FFFFFF!important;; display:inline-block; border:0 !important; transition:ease .2s background-color; border-radius:4px}
.btn-blue-s2:hover{background-color: #81bcf2;}


.tab{ height:44px; line-height: 44px; display:block; background:url(../images/tab-bg.png) repeat-x bottom; margin-bottom:20px; } 
.tab li a{ float:left;display:block; padding:0 15px; }
.tab-li-crr a{ background:url(../images/tab-hover-bg.png) top repeat-x; height:44px; line-height:44px; display:block; padding:0 15px;border-left:1px solid #EEEEEE; border-right:1px solid #EEEEEE; color:#333333 !important}
.table-head{ border-bottom:2px solid #c4e5fe !important; border-top:0 !important;background-color: #f5fafe; height:42px; line-height:42px;}

.search-table{ margin-bottom:10px; font-weight:normal}
.search-table td{ height:50px; line-height:50px; padding:0 5px;}

.mt-10{ margin-top:20px; color:#666;}

/*form*/
.form-table th{ height:40px; line-height: 40px;font-weight:normal}
.form-table td{ height:40px; line-height: 40px;}
.form-font-normal{ font-weight:normal;}
.form-input-w300px{ width:300px;}
.form-input-w500px{ width:500px;}
form label{ font-weight:normal;}
.form-group-title{font-weight:bold;margin-bottom:20px;}
.red{color:#a94442;}
.list-number{text-align:right;}

.langActive{color: #ff8315 !important;}

/* tab页面    add wwluo 160808 */
.tab-side { width: 100%; background: #FFF; height: 100%; float: left; }
.tab-side-container { padding:15px; }
.ui-icon { width: 16px; height: 16px; display: block; background-repeat: no-repeat; overflow: hidden; background-image: url("../images/ui-icons_454545_256x240.png"); }
.ui-icon-close { opacity: 0.2; float: right; cursor: pointer; margin: 0.4em -10px 0 0.6em; background-position: -80px -128px; }
.ui-icon-close:hover{ opacity: 1; background-position: -30px -190px; }

.btn-primary{background:#d84a38;border:none;border:0;}
.btn-primary:hover{background:#bb2413;}
