@charset "utf-8";
/* CSS Document */
*{ padding:0; margin:0;}
ul li{ list-style-type:none;padding:0; margin:0;}
body{font-family:'MicrosoftYaHei', Helvetica,  sans-serif; font-size:1em;   }
.clear{ clear:both;}
a{text-decoration:none; color:#2095d5}
a:hover{ color:#35b3f7}
.anima-box{ height: 100vh; width:100%; display:block;  background: linear-gradient(180deg, #006eaa, #002455); }
.l1{ background:url(../images/l1.png) no-repeat center left; width:100%; height:60px; background-size:100% 100%; position:absolute;z-index:41;animation:d1 3s ease-in-out 200; top:10px;}
.l2{ background:url(../images/l2.png) no-repeat center left; width:100%; height:69px;background-size:100% 100%;position: absolute;z-index:42;animation:d2 3s ease-in-out 200;top:20px;}
.l3{ background:url(../images/l3.png) no-repeat center left; width:100%;height:89px; background-size:100% 100%;position:absolute;z-index:43;animation:d3 3s ease-in-out 200;top:30px;}

.wmes{ background:url(wmes.png) no-repeat left top; width:502px; height:121px ;z-index:55; position:relative; top:250px; margin:auto;animation:wmes 1s ease 1; }

.wmes-txt{ width:458px;  top:250px;z-index:58;position:relative; margin:auto;animation:wmes-txt 1s ease 1; }

.sm{ width:150px; top:80px; height:48px; display:block; z-index:59; margin:auto; position:relative;animation:sm 1s ease-in-out 1; }

@keyframes d1
{
0%   {top:480px;}
50%  {top:460px;}
100% {top:480px;}
}

@keyframes d2
{
0%   {top:470px;}
50%  {top:460px;}
100% {top:470px;}
}

@keyframes d3
{
0%   {top:410px;}
50%  {top:440px;}
100% {top:410px;}
}

@keyframes wmes
{
0%   { opacity:0; top:230px;}
100% { opacity:1; top:250px;}
}

@keyframes wmes-txt
{
0%   { opacity:0; top:260px;}
100% { opacity:1; top:250px;}
}

@keyframes sm
{
0%   { opacity:0; top:70px;animation-delay:2s;}
100% { opacity:1; top:80px;}
}



.logo{ width:50px; height:50px;left:10px; z-index:62; padding:30px; position:absolute; animation:sm-logo 30s ease-in-out 221;}
.list{ overflow:hidden}
.list li{ display:block;  height:100vh;vertical-align:middle; text-align:center; font-family: Verdana; color:#eaeaea; float:left; width:24%; background-color:transparent; animation:all 0.2s ease-in; transition:all 1s }

.list li:hover{ display:block; background-color:#ffffff;width:28%; color:#000000 !important}

.t-link{ font-size:3em; margin-top:50vh;  display:block;line-height:5vh;vertical-align: middle; color:#8dccef;transition:all 0.2s; text-decoration:none;}
.list li a:hover{ color:#FF9900 ; }
.t-link-small-tx{ font-size:1em !important; margin-top:20px}



@keyframes sm-logo
{
0%   {  left:10px;top:10px;}
2%   {  left:-55px;top:10px; animation:ease-out}
14%  {  left:-55px;top:10px; animation:ease-out}
15%  {  left:10px;top:10px;animation:ease-in}
50%  {  left:10px;top:10px;animation:ease-in}
51%  {  left:10px;top:-15px;animation:ease-in}
52%  {  left:10px;top:10px;animation:ease-in}
100% {  left:10px;top:10px;animation:ease-in}
}



/*层效果*/

.layer-box{ height:100vh; width:100%; margin:0 auto; background:#006eaa; }
.layer-box div{ box-shadow:#888888 1px 1px 2px;background-color:#FFF; width:500px; height:400px; margin:20px; padding:20px; display:block; left:30px; top:50px;}
#layer1{ position:absolute; background-color:#FFF; left:110px; top:30px;}
#layer2{ position:absolute; background-color:#FFF; left:180px; top:90px;}
#layer3{ position:absolute; background-color:#FFF; left:250px; top:150px;}


/*top5*/
.left-s-menu{ width:100px; height:100vh;  background:#FFFFFF; border-right:1px solid #eaeaea; position:absolute; z-index:100; float:left;transition:all 0.5s}
.left-s-menu-out{ width:200px;  height:100vh; background:#FFFFFF;border-right:1px solid #eaeaea;z-index:100; float:left; transition:all 0.5s; box-shadow:1px 1px 5px #666666}

.ls-menu{ font-size:1em; }
.li-menu li{ width:180px; }
.ls-menu li a{ color:#2095d5;padding:0 10px; line-height:40px; height:40px;display:block;  transition:all 0.3s ease }

.ls-menu li a:hover{ background: #cbe9f9; color:#2095d5}

.ls-menu-li-crr{ border-left:4px solid #ff9900; color:#2095d5 ; background:#cbe9f9; font-weight:bold;}

.top5-menu-close{width:50px; margin:0 auto;height:50px;text-align:center; background: url(../images/top5-open.png) center no-repeat; animation:all ease-in; transition: all  1s; transform:rotate(0deg); display:block}
.top5-menu-close:hover{ transform:rotate(360deg)}

.top5-menu-open{ width:50px;margin:0 auto;height:50px;text-align:center; background: url(../images/top5-close.png) center no-repeat; animation:all ease-in; transition: all  1s; transform:rotate(0deg); display:block}
.top5-menu-open:hover{ transform:rotate(360deg)}
.top5-btn{ border-top:1px solid #EAEAEA;}
.top5-logo{width:50px; height:50px; padding:15px; margin:0 auto; }
.top5-top{ color:#333333; background:#ffffff; text-align:center; height:80px; line-height:80px;}

.right-s-content{width:100%; height:100vh; float:left; display:block; background:#e1eef7; }
.top5-content{}
.top5-content-l{ margin-left: 120px; margin-top:26px;display:block; float:left;width:400px;  text-align:center; color:#FFFFFF;background: #006eaa;}

.top5-content-l a{ border:1px solid #FFFFFF; padding:10px 20px; color:#FFFFFF !important; border-radius:5px; display:inline-block; transition:all 0.3s ease}
.top5-content-l a:hover{ border:1px solid #a8e0ff; padding:10px 30px; color:#ffffff; border-radius:5px; display:inline-block;}

.top5-content-l p{ padding:10px; font-size:1.5em}
.top5-content-l h1{ font-size:4em}
.top5-content-r{ display:block; margin-left:530px;  padding-top:20px; transition:all 0.5s ease}
.top5-bar{  margin:0 10px;}
.top5-bar ul li{width:auto;  height:90px; margin:10px 0; display:block; }
.bar-bg{height:30px; width:100%;display:block; background:#cbe9f9;}
.bar{height:30px; background:#006eaa; display:block;}
.top5-bar-li{ width:100%; height:80px;  font-size:1em}
.top5-bar-num{ font-family:"微软雅黑"; width:50px; height:80px;line-height: 80px; font-size:2em; text-align:center; float:left; display:block}
.top5-bar-text{ padding-left:50px; display:block}
.top5-bar-tx1,top-bar-tx2{height:30px; line-height: 30px; text-align:left !important; }

.top5-bar-tx1-pe-1{ float:left;}
.top5-bar-tx1-pe-2{ font-size: 1.5em; float:right;}
.top5-bar-tx1-pe-3{ font-size: .75em; float:right;}

.top5-w100{ width:100% !important;}
.top5-w66{ width:66% !important;}
.top5-w50{ width:50% !important;}
.top5-w26{ width:26% !important;}
.top5-w20{ width:20% !important;}

.top5-tip{ z-index:115; position: absolute; top:360px; left:240px; display:none}
.top5-list-tip{ z-index:116; position:absolute; margin-left:-66px;display:none; margin-top:-10px;}
.top5-list-tip2{ position: absolute; text-align:left; margin-left:10px;display:none; margin-top:20px;}
.top5-title{ font-size:26px; font-weight:bold;}
.c-666{ color:#666666; float:right; padding-right:15px;}
.top5-top a{ color:#2095d5; font-weight: normal; font-size:0.75em !important;}

.foi-content{ float:left;}

.foi-l-content{float:left; padding-left:150px;  }

.foi-l-box{ display:box;display:-webkit-box;display:-moz-box;display:-ms-box}
.foi-l-box-1{  width:150px;}
.foi-l-box-2{ box-flex:1;-webkit-box-flex:1;-moz-box-flex:1;-ms-box-flex:1}
.foi-l-box-3{ width:250px; background:#FF9900;}


.foi-r-menu{ float:right; height:80vh;background:#006eaa; }

.foi-l-content span{ float:left; width:30%; margin:10px;height:200px; line-height: 200px; text-align:center; border:1px solid #EDEDED; border:1px solid #FFFFFF}
.foi-l-content  a{ display:block;}
.foi-r-menu-btn{ display:block; width:160px; height:40px;line-height: 40px; background:#FFFFFF; margin:10px auto; text-align:center;}






