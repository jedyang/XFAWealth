.topicDetail-ico-word{float: left;margin-left: 25px;}
.topicDetail-ico-word > img{margin-right: 5px;}
.topicDetail-rows-reply{overflow: hidden;margin-left: 85px;background-color: #fafafa;padding: 15px 20px;margin-top: 10px;margin-bottom: 20px;}
.topicDetail-reply{overflow: hidden;}
.topicDetail-reply-portrait{border-radius: 50%;width: 63px;float: left;}
.topicDetail-reply-name{margin-left: 80px;color: #999999;margin-bottom: 10px;}
.topicDetail-reply-word{margin-left: 95px;height: 50px;overflow: hidden;margin-bottom: 5px;}
.topicDetail-add-iof{float: left;}
.topicDetail-add-button{background: #ff9019;color: white;padding: 6px 16px;border-radius: 4px;float: right;}
.topicDetail-add-wrap{overflow: hidden;}
.topicDetail-add-text{margin: 0 auto;margin-bottom: 14px;height: 100px;resize: none;border: #e5e4e4 1px solid;width: 99%;display: block;}
.topicDetail-add-name{text-align: center;margin-top: 10px;}
.topicDetail-add-ico{float: left;margin-left: 7px;}
.topicDetail-add-content{margin-left: 105px;}
.messages-wrapper{    background-color: white;padding: 35px 40px;margin-bottom: 20px;position: relative;overflow: hidden;}
.messages-wrapper-list > li{overflow: hidden;background-color: #fbf7fc;margin-bottom: 20px;padding: 25px 20px;}
.messages-wrapper-list > li:nth-child(2n){background-color: #fafcf7;}
.messages-list-ico{float: left;}
.messages-list-rows{margin-left: 65px;}
.messages-list-title{font-size: 16px;margin-bottom: 5px;}
.messages-list-word{font-size: 16px;margin-bottom: 20px;}
.messages-list-time{color: #999999;}

.discover-chat-mask{width: 100%;padding-left: 100px;box-sizing: border-box;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;}
.discover-space-popup{display: none;}
.discover-space-active{display: block;}
.discover-chat-wrap{padding: 15px 20px;overflow: hidden;position: relative;}
.discover-chat-ico{position: absolute;right: 12px;top: 20px;}
.discover-chat-wrap img{float: right;margin: 0px 10px;cursor: pointer;}
.discover-chat-contents{padding: 20px 10px;}
.discover-own-msg{overflow: hidden;margin-bottom: 20px;}
.discover-own-contens{float: right;text-align: left;background-color: #7bcbfd;color: white;padding: 10px;margin: 11px 0px;margin-right: 15px;position: relative;max-width: 70%;}
.discover-own-contens:before{content: "";display: block;border-color: transparent;border-width: 7px;border-style: solid;width: 0px;height: 0px;position: absolute;top: 11px;right: -14px;border-left-color: #7bcbfd;}
.discover-own-portrait{float: right;width: 60px;border-radius: 50%;}
.discover-others-msg{overflow: hidden;margin-bottom: 20px;}
.discover-others-contens{float: left;text-align: left;background-color: white;color: #000000;padding: 10px;margin: 11px 0px;margin-left: 15px;position: relative;max-width: 70%;}
.discover-others-contens:before{content: "";display: block;border-color: transparent;border-width: 7px;border-style: solid;width: 0px;height: 0px;position: absolute;top: 18px;left: -14px;border-right-color: #ffffff;}
.discover-others-portrait{float: left;width: 60px;border-radius: 50%;}
.discover-chat-time{color: #cccccc;margin: 10px 0px;}
.discover-chat-add{position: relative;overflow: hidden;text-align: center;padding-top: 10px;width: 100%;}
.discover-chat-text{width: 99%;margin: 0 auto;height: 100px;resize: none;border: #e5e4e4 1px solid;}
.discover-chat-button{position: absolute;right: -5px;bottom: 9px;color: #999999;padding: 6px 16px;margin-right: 15px;border-radius: 4px;border: 1px solid #edf0ee;}
.chat-expression-ico{position: absolute;top: 21px;left: 10px;cursor: pointer;}


/*discover live*/
.dlive-wrapper{background-color: #f5f5f5;}
.live-viedo-wrapper{background-color: black;}


.conner-rows-ico ul{z-index: 2;display: none;border: 1px solid #e0dfdf;width: 138px;top: 27px;}
.conner-rows-ico ul li{padding: 8px 5px;background: white;cursor: pointer;color: #606060;}
.conner-rows-ico ul li:hover{  background-color: #0255C0;color: white;}
.conner-rows-ico-show{display: block!important;}