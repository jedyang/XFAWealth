/*choice*/
.wmes_choice_wrap{padding: 10px 25px;}
.wmes_choice_wrap:after {clear:both;content:'.';display:block;width: 0;height: 0;visibility:hidden;} 
.wmes_choice_title{color: #606060;width: 115px;height: 30px;line-height: 30px;float: left;}

.wmes_choice:after {clear:both;content:'.';display:block;width: 0;height: 0;visibility:hidden;}
.wmes_choice li{color: #000000;cursor: pointer;display: inline-block;height: 30px;line-height: 30px;min-width: 15px;padding: 0px 5px;text-align: center;margin: 0px 5px;margin-bottom: 5px;border-radius: 3px;}
.wmes_choice li:nth-child(1){min-width: 50px;}
.wmes_choice .wmes_choice_active{background-color: #36b0fd;color: white;}

/*close*/
.wmes-close{transition:transform 0.5s;;border-radius:4px;position: absolute;background: 0 0;right: 30px;cursor: pointer;height: 30px;width: 30px;top: 10px;right: 10px;}
.wmes-close:after, .wmes-close:before {content: '';background: #999999;height: 2px;width: 15px;}
.wmes-close:after{-moz-transform: translate(0,-6px) rotate(45deg);-ms-transform: translate(0,-6px) rotate(45deg);-webkit-transform: translate(0,-6px) rotate(45deg);transform: translate(0,-6px) rotate(45deg);position: absolute;bottom: 8px;left: 8px;}
.wmes-close:before{-moz-transform: translate(0,6px) rotate(-45deg);-ms-transform: translate(0,6px) rotate(-45deg);-webkit-transform: translate(0,6px) rotate(-45deg);transform: translate(0,6px) rotate(-45deg);position: absolute;top: 8px;left: 8px;}
.wmes-close:hover:after, .wmes-close:hover:before{background:#fff;}
.wmes-close:hover{transform:rotate(180deg);background:#427cb2;border-radius:50%;}