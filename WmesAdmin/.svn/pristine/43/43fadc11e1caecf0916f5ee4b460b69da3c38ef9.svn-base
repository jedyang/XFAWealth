<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>[#if fivo?exists && fivo.id?exists] [@lang_res k='member.fi'/] [@lang_res k='member.detail'/] [#else] [@lang_res k='member.fi'/] [@lang_res k='member.add'/] [/#if]</title>
		<link href="${base}/res/console/css/bootstrap-3.3.4.css" rel="stylesheet" type="text/css">
		<link href="${base}/res/console/css/css.css" rel="stylesheet" type="text/css">
		<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
		<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
		<!--[if lt IE 9]>
 		<script src="${base}/res/js/html5shiv.min.js"></script>
	  	<script src="${base}/res/js/respond.min.js"></script>
	    <![endif]-->
		<script src="${base}/res/console/js/jquery-1.11.2.min.js"></script>
		<script src="${base}/res/console/js/jquery.form.js"></script>
		<script src="${base}/res/console/js/bootstrap.js"></script>
		<script src="${base}/res/third/bootstrap/js/bootstrapValidator.js"></script>
		<script src="${base}/res/console/js/js.js" type="text/javascript"></script>
		<script src="${base}/res/third/laydate/laydate.js" type="text/javascript"></script>
		<script src="${base}/res/third/layer/layer_v2.js"></script>
	</head>
	<body>

		<div class="container-fluid pd0" id="wmes_wraper">
			<div style="padding-left: 20px; padding-right: 20px;">
				<div class="page-header mt-10">
					<h4>
						<b>[#if fivo?exists && fivo.id?exists] [@lang_res k='member.fi'/] [@lang_res k='member.detail'/] [#else] [@lang_res k='member.fi'/] [@lang_res k='member.add'/] [/#if]</b>
					</h4>
				</div>
				<div>
					<form id="frm" class="form-horizontal" method="post">
						<input type="hidden" id="hidCountry" value="${(fivo.country)?if_exists}">
						<input type="hidden" id="hidIncorporationPlace" value="${(fivo.incorporationPlace)?if_exists}">
						<input type="hidden" id="hidIndividualId" name="id" value="${(fivo.id)?if_exists}">
						<input type="hidden" id="hidIsFinancial" value="${(fivo.isFinancial)?if_exists}">
						<input type="hidden" id="hidEntityType" value="${(fivo.entityType)?if_exists}">
						<input type="hidden" id="hidMemberId" value="${(fivo.memberId)?if_exists}">
						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.loginCode'/]
							</label>
							<div class="col-md-4">
								<input id='txtLoginCode' type="text" class="form-control " maxlength="50" name='loginCode' value="${(fivo.loginCode)?if_exists}">
							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.nickName'/]
							</label>
							<div class="col-md-4">
								<input id="txtNickName" type="text" class="form-control " maxlength="50" name="nickName" value="${(fivo.nickName)?if_exists}">
							</div>

						</div>

						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.email'/]
							</label>
							<div class="col-md-4">
								<input id='txtEmail' type="text" class="form-control " maxlength="50" name="email" value="${(fivo.email)?if_exists}">
							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								11111111[@lang_res k='member.info.mobileNumber'/]
							</label>
							<div class="col-md-4">
								<input id='txtPhone' type="text" class="form-control " maxlength="20" name="mobileNumber" value="${(fivo.mobileNumber)?if_exists}">
							</div>
						</div>


						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.companyName'/]
							</label>
							<div class="col-md-4">
								<input id="txtCompanyName" type="text" class="form-control " maxlength="100" name="companyName" value="${(fivo.companyName)?if_exists}">
							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.entityType'/]
							</label>
							<div class="col-md-4">
								<select id="dllCntityType" name="entityType" class="form-control " value="${(fivo.entityType)?if_exists}">
									<option value="">
										[@lang_res k='global.pleaseSelect'/]
									</option>
									<option value="1">
										[@lang_res k='member.info.entityType.PrivateLtdCo'/]
									</option>
									<option value="2">
										[@lang_res k='member.info.entityType.PrivateCo'/]
									</option>
									<option value="3">
										[@lang_res k='member.info.entityType.TrustAccount'/]
									</option>
									<option value="4">
										[@lang_res k='member.info.entityType.ListedCo'/]
									</option>
									<option value="0">
										[@lang_res k='member.info.entityType.Other'/]
									</option>
								</select>

							</div>
						</div>


						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.entityOther'/]
							</label>
							<div class="col-md-4">
								<input id="txtEntityOther" type="text" class="form-control " maxlength="200" name="entityOther" value="${(fivo.entityOther)?if_exists}">
							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.registerNo'/]
							</label>
							<div class="col-md-4">
								<input id="txtRegisterNo" type="text" class="form-control " maxlength="25" name="registerNo" value="${(fivo.registerNo)?if_exists}">
							</div>
						</div>



						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.isFinancial'/]
							</label>
							<div class="col-md-4">
								<select id="ddlIsFinancial" name="isFinancial" class="form-control " value="${(fivo.isFinancial)?if_exists}">
									<option value="">
										[@lang_res k='global.pleaseSelect'/]
									</option>
									<option value="0">
										[@lang_res k='global.true'/]
									</option>
									<option value="1">
										[@lang_res k='global.false'/]
									</option>
								</select>

							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.giin'/]
							</label>
							<div class="col-md-4">
								<input id="txtGiin" type="text" class="form-control " maxlength="50" name="giin" value="${(fivo.giin)?if_exists}">
							</div>
						</div>



						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.naturePurpose'/]
							</label>
							<div class="col-md-4">
								<input id="txtNaturePurpose" type="text" class="form-control " maxlength="50" maxlength="250" name="naturePurpose" value="${(fivo.naturePurpose)?if_exists}">
							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.incorporationDate'/]
							</label>
							<div class="col-md-4">
								<input id='txtIncorporationDate' type="text" class="laydate-icon form-control form-control-laydate" style="height: 34px; padding: 6px 12px;" onclick="laydate({istime: false, format: 'YYYY-MM-DD'})" name="incorporationDate" value="${(fivo.incorporationDate)?if_exists}">
							</div>
						</div>



						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.incorporationPlace'/]
							</label>
							<div class="col-md-4">
								<select id="ddlIncorporationPlace" name="incorporationPlace" class="form-control " value="${(fivo.incorporationPlace)?if_exists}">
									<option value="">
										[@lang_res k='global.pleaseSelect'/]
									</option>
								</select>
							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.registeredAddress'/]
							</label>
							<div class="col-md-4">
								<input id='txtRegisteredAddress' type="text" class="form-control " maxlength="250" name="registeredAddress" value="${(fivo.registeredAddress)?if_exists}">
							</div>
						</div>



						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.mailingAddress'/]
							</label>
							<div class="col-md-4">
								<input id='txtMailingAddress' type="text" class="form-control " maxlength="200" name="mailingAddress" value="${(fivo.mailingAddress)?if_exists}">
							</div>
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.country'/]
							</label>
							<div class="col-md-4">
								<select id="ddlCountry" class="form-control " name="country" value="${(fivo.country)?if_exists}">
									<option value="">
										[@lang_res k='global.pleaseSelect'/]
									</option>
								</select>
							</div>
						</div>



						<div class="form-group" style="margin-bottom: 5px">
							<label class="col-md-2 control-label " style="font-weight: normal; text-align: right;" for="ds_password">
								[@lang_res k='member.info.website'/]
							</label>
							<div class="col-md-4">
								<input id='txtWebsite' type="text" class="form-control " name="website" maxlength="100" value="${(fivo.website)?if_exists}">
							</div>

						</div>
						<div class="text-center pd10">
							<button id="btnBack" class="btn btn-primary" onclick="javascript:closeTab();">
								[@lang_res k='global.close'/]
							</button>
						</div>
					</form>

				</div>
			</div>
		</div>

	</body>

	<script type="text/javascript">
	$(function() {
		bindCountry();

		$("#ddlIsFinancial").val($("#hidIsFinancial").val());
		$("#dllCntityType").val($("#hidEntityType").val());
		$("#frm").bootstrapValidator({
			feedbackIcons : {
				valid : 'glyphicon glyphicon-ok',
				invalid : 'glyphicon glyphicon-remove',
				validating : 'glyphicon glyphicon-refresh'
			},
			fields : {
				loginCode : {
					message : '登录帐号还没有验证',
					validators : {
						notEmpty : {
							message : '登录帐号不能为空'
						},
						callback : {
							message : '帐号已被使用',
							callback : function(value, validate) {
								var url = "${base}/console/member/checkCodeUnique.do?datestr=" + new Date().getTime() + "&loginCode=" + encodeURI(encodeURI(value)) + "&userId=" + encodeURI(encodeURI($("#hidMemberId").val()));
								var res = true;
								/*if ($("#txtLoginCode").val() != value
										&& "" != value) {*/
								$.ajax({
									url : url,
									method : 'post',
									dataType : 'json',
									async : false,
									success : function(data) {
										res = data;
									},
									error : function() {
									}
								});
								//}
								return res;
							}
						}
					}
				}
			}
		}).on('success.form.bv', function(e) {
			e.preventDefault();
			//alert('valid ok');
			Save();
		});
	});
	function Save() {

		var params = $("#frm").serialize();
		params = decodeURIComponent(params, true);

		$.ajax({
			type : 'post',
			datatype : 'json',
			url : "${base}/console/member/fi/save.do?datestr=" + new Date().getTime(),
			data : {
				postData : params
			},
			success : function(data) {
				//var dataObj = $.parseJSON(data.replace(/<.*?>/ig, ""));
				if (data.result) {
					layer.msg("[@lang_res k='global.success.save'/]", {
						icon : 1,
						time : 1000
					});
					window.location.href = "${base}/console/member/list.do"
				} else {
					$('#contact-form-error').show().fadeOut(10000);
				}
			}
		});

		/*$("#frm").ajaxSubmit({
			url : "${base}/console/member/fi/save.do?datestr=" + new Date().getTime(),
			iframe : true,
			success : function(data, status) {
				var dataObj = $.parseJSON(data.replace(/<.*?>/ig, ""));
				if (dataObj.result) {
					window.location.href = "${base}/console/member/list.do"
				} else {
					$('#contact-form-error').show().fadeOut(10000);
				}
			},
			error : function() {
				alder("失败");
				$('#contact-form-error').show().fadeOut(10000);
			}
		});*/
	}

	function bindCountry() {
		$.ajax({
			type : 'post',
			datatype : 'json',
			url : "${base}/console/country/langListJson.do?datestr=" + new Date().getTime(),
			data : {},
			success : function(json) {
				if (json != undefined && json.result == true) {
					var data = JSON.parse(json.countryJson);
					var html = "";
					$.each(data, function(i, n) {
						html += "<option value='"+n.id+"'>" + n.name + "</option>";
					});

					$("#ddlIncorporationPlace").append(html);
					$("#ddlCountry").append(html);
					$('#ddlCountry').val($('#hidCountry').val());
					$('#ddlIncorporationPlace').val($('#hidIncorporationPlace').val());
				}

			}
		});
	}
</script>
</html>