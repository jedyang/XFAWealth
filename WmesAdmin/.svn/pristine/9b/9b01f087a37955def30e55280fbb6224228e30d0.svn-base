<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Company Info</title>
    <link href="${base}/res/console/css/bootstrap-3.3.4.css" rel="stylesheet" type="text/css">
    <link href="${base}/res/console/css/css.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="${base}/res/css/wmesupload.css"> 
    <script type="text/javascript">
        var base_root = '${base}';
    </script>
    <script type="text/javascript">
       var props = [@load_prop k='console.info.uploadImage'/];
	</script>
</head>

<body>
<div class="container-fluid pd0" id="wmes_wraper">
    <div class="tab-side container-fluid">
        <div class="tab-side-container">
            <div class="page-header mt-10">
                <h4><b>[@lang_res k='console.company.title'/]</b></h4>
            </div>
            <ul class="tab" id="tab-search">
                <li class="tab-li-crr">
                    <a href="javascript:;">[@lang_res k='console.rpq.questForm.scBasicInfo'/]</a>
                </li>
                <li class="tab-li">
                    <a href="javascript:;">[@lang_res k='console.rpq.questForm.tcBasicInfo'/]</a>
                </li>
                <li class="tab-li">
                    <a href="javascript:;">[@lang_res k='console.rpq.questForm.enBasicInfo'/]</a>
                </li>
            </ul>
               <div class="clear"></div>
               <div class="tab_wrap">
                <!-- SC -->
                <form class="form-horizontal" style="display:block;">
                    <!-- ID
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtId" class="label-control">ID</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtId" readonly="readonly" value="${(infoVO.id)?if_exists}" >
                        </div>
                    </div> -->
                    <!-- 企业编码 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtCode" class="label-control"><span class="red">*</span>[@lang_res k='console.company.code'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtCode" value="${(infoVO.code)?if_exists}" >
                        </div>
                    </div>     
                    <!-- 企业名称 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtNameSc" class="label-control"><span class="red">*</span>[@lang_res k='console.company.name'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtNameSc" value="${(infoVO.companyInfoSc.name)?if_exists}" >
                        </div>
                    </div>
                    <!-- 系统名称 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtSysNameSc" class="label-control">[@lang_res k='console.company.sysName'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtSysNameSc" value="${(infoVO.companyInfoSc.sysName)?if_exists}" >
                        </div>
                    </div>
                    <!-- 版权声明 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtCopyrightSc" class="label-control">[@lang_res k='console.company.copyright'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtCopyrightSc" value="${(infoVO.companyInfoSc.copyright)?if_exists}" >
                        </div>
                    </div>
                    <!-- Web地址 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtWebUrl" class="label-control">[@lang_res k='console.company.webUrl'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtWebUrl" value="${(infoVO.webUrl)?if_exists}" >
                        </div>
                    </div>
                    <!-- logo图标 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="imgLogoUrl" class="label-control"><span class="red">*</span>[@lang_res k='console.company.logoUrl'/]</label>
                        </div>
                        <div class="col-sm-7" style="vertical-align: top;">
                            <div class="upload-box upload-album" id="logo"></div>
                            <span>[@lang_res k='console.company.imageTips'/]</span>
                            <br>
                            <img id="imgLogoUrl" style="width:151px;height:45px;" value="${infoVO.logoUrl!}" src="${base}[@user_head u='${infoVO.logoUrl!}' t='c'/]"/>
                            <!-- <input type="text" class="form-control" id="imgLogoUrl" value="${(infoVO.logoUrl)?if_exists}" > -->
                        </div>
                    </div>
                    <!-- 背景图 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="imgBackgroundUrl" class="label-control"><span class="red">*</span>[@lang_res k='console.company.backgroundUrl'/]</label>
                        </div>
                        <div class="col-sm-7" style="vertical-align: top;">
                            <div class="upload-box upload-album" id="background"></div>
                            <span>[@lang_res k='console.company.backgroundImg'/]</span>
                            <br>
                            <img id="imgBackgroundUrl" style="width:222px;height:80px;" value="${infoVO.backgroundUrl!}" src="${base}[@user_head u='${infoVO.backgroundUrl!}' t='c'/]"/>
                            <!-- <input type="text" class="form-control" id="imgBackgroundUrl" value="${(infoVO.backgroundUrl)?if_exists}" > -->
                        </div>
                    </div>
                    <!-- 主样式 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtCssUrl" class="label-control">[@lang_res k='console.company.cssUrl'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtCssUrl" value="${(infoVO.cssUrl)?if_exists}" >
                        </div>
                    </div>
                    <!-- 登录页 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtLoginUrl" class="label-control">[@lang_res k='console.company.loginUrl'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtLoginUrl" value="${(infoVO.loginUrl)?if_exists}" >
                        </div>
                    </div>
                </form>
                <!-- TC -->
                <form class="form-horizontal" style="display:none;">    
                    <!-- 企业名称 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtNameTc" class="label-control"><span class="red">*</span>[@lang_res k='console.company.name'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtNameTc" value="${(infoVO.companyInfoTc.name)?if_exists}" >
                        </div>
                    </div>
                    <!-- 系统名称 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtSysNameTc" class="label-control">[@lang_res k='console.company.sysName'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtSysNameTc" value="${(infoVO.companyInfoTc.sysName)?if_exists}" >
                        </div>
                    </div>
                    <!-- 版权声明 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtCopyrightTc" class="label-control">[@lang_res k='console.company.copyright'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtCopyrightTc" value="${(infoVO.companyInfoTc.copyright)?if_exists}" >
                        </div>
                    </div>
                </form>
                <!-- EN -->
                <form class="form-horizontal" style="display:none;">  
                    <!-- 企业名称 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtNameEn" class="label-control"><span class="red">*</span>[@lang_res k='console.company.name'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtNameEn" value="${(infoVO.companyInfoEn.name)?if_exists}" >
                        </div>
                    </div>
                    <!-- 系统名称 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtSysNameEn" class="label-control">[@lang_res k='console.company.sysName'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtSysNameEn" value="${(infoVO.companyInfoEn.sysName)?if_exists}" >
                        </div>
                    </div>
                    <!-- 版权声明 -->
                    <div class="form-group">
                        <div class="col-sm-3 text-right">
                            <label for="txtCopyrightEn" class="label-control">[@lang_res k='console.company.copyright'/]</label>
                        </div>
                        <div class="col-sm-7">
                            <input type="text" class="form-control" id="txtCopyrightEn" value="${(infoVO.companyInfoEn.copyright)?if_exists}" >
                        </div>
                    </div>
                </form>
                <!--<div class="text-center pd10">
                    <a href="#" onclick="closeTab();" class="btn-blue-s2">[@lang_res k='global.close'/]</a>
                </div>
           <div class="clear"></div>-->
            </div>
        </div>
        <div class="text-center pd10">
            <button id="btnSave" class="btn-blue-s" >
                [@lang_res k='global.save'/]
            </button>
        </div> 
    </div>
</div>
<script src="${base}/res/console/js/jquery-1.11.2.min.js" type="text/javascript"></script>
<script src="${base}/res/console/js/bootstrap.js" type="text/javascript"></script>
<script src="${base}/res/third/jqueryui/jquery-ui.js" type="text/javascript"></script>
<script src="${base}/res/console/js/js.js" type="text/javascript"></script>
<script src="${base}/res/third/layer/layer_v2.js"></script>
<script src="${base}/res/js/util.js"></script>
<script type="text/javascript" src="${base}/res/js/base.js"></script>
<script type="text/javascript" src="${base}/res/js/seajs/sea.js"></script>
<script type="text/javascript" src="${base}/res/js/config-sea.js"></script>
<script type="text/javascript">
var langMutilForJs = [@load_prop k='global'/];
seajs.use('${base}/res/console/web/company/input.js');
var id = getQueryString('id');
var lang
</script>
</body>
</html>
