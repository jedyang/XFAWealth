.order-setting-top{background-color: #f8f8f8;overflow: hidden;border-bottom: 1px solid #e8e8e8;line-height: 28px;font-weight: bold;padding: 3px 0px 3px 10px;margin-top: 30px;}
.order-setting-title{float: left;}
.order-setting-xiala{width: 16px;height: 16px;background: url('../images/ico_xiala.png') no-repeat;float: right;margin: 6px 7px;cursor: pointer;}
.order-setting-contents{margin-top: 30px;}
.order-setting-contents:after {clear:both;content:'.';display:block;width: 0;height: 0;visibility:hidden;}
.order-setting-left{width: 55%;float: left;border-right: 1px solid #e3e3e3;box-sizing: border-box;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;}
.order-setting-list{line-height: 30px;}
.order-setting-rows{overflow: hidden;margin-bottom: 20px;}
.order-setting-list-name{float: left;color: #999999;width: 100px;text-align: right;margin-right: 15px;}
.order-setting-cycle{float: left;line-height: 20px;margin: 0px 3px;padding-bottom: 5px;cursor: pointer;}
.order-setting-cycle > span{padding: 0px 15px;}
.order-setting-cycle:nth-child(3) span{border-left: 1px solid #e3e3e3;border-right: 1px solid #e3e3e3;}
.order-setting-cdate{float: left;border: 1px solid #e3e3e3;width: 100px;padding: 5px 5px;}
.order-setting-cdate > option{padding: 5px 0px;}
.order-setting-last-word{float: left;margin-left: 25px;color: #999999;}
.order-setting-last-word span{color: black;margin-left: 10px;}
.order-money-input{float: left;border: 1px solid #e3e3e3;width: 90px;padding: 5px 5px;}
.order-setting-list-cur{float: left;margin-left: 10px;}
.order-setting-right{float: left;width: 45%;}
.order-cycle-active{color: #10a1ec;border-bottom: 2px solid #10a1ec;}
.order-choice-date{float: right;display: none;}
.order-choice-title{color: #999999;float: left;line-height: 34px;margin-right: 10px;}
#order-choice{width: 150px;height: 30px;border: #dcdadb 1px solid;border-radius: 3px;font-size: 14px;padding-left: 5px;background-image: url('../images/ico_date01.png');background-position: 95%;background-repeat: no-repeat;}
#order-number-choice{width: 50px;height: 30px;border: #dcdadb 1px solid;border-radius: 3px;font-size: 14px;padding-left: 5px;}
#order-money-choice{width: 150px;height: 30px;border: #dcdadb 1px solid;border-radius: 3px;font-size: 14px;padding-left: 5px;}
.order-plan-rows .client-more-ico{margin: 0px;}
.order-table{margin-top: 25px;}
.client-table .order-tables-name{padding-left: 30px;position: relative;text-align: left;}
.order-tables-warning{position: absolute;left: 10px;top: 40%;}
.order-table-select{width: 100%;padding: 5px 0px;}
.order-textarea-title{text-align: left;margin-bottom: 15px;}
.order-textarea-text{margin: 0 auto;margin-bottom: 14px;height: 100px;resize: none;border: #e5e4e4 1px solid;width: 100%;display: block;}
.order-textarea-rows{padding: 0px 45px;}
.order-plan-button{text-align:right;margin-top: 50px;margin-bottom: 60px;}
.order-button-reject{background-color: #eb1840;color: white;padding: 5px 20px;border-radius: 5px;}
.order-button-approved{background-color: #2d80ce;color: white;padding: 5px 20px;border-radius: 5px;margin: 0px 15px;}
.order-button-close{background-color: #919599;color: white;padding: 5px 20px;border-radius: 5px;}
.order_list_paging{text-align: center;}
.order_list_paging > li{display: inline-block;}
.order_pagint_num{width: 35px;height: 35px;line-height: 35px;background-color: white;border-radius: 50%;margin: 0px 1px;border: 1px solid #e4e4e4;}
.order_pagint_now{background-color: #2d80ce;color: white;}
.order-plan-sapce{display: none;position: absolute;box-sizing: border-box;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;top: 20px;left: 130px;}
.order-plan-block{display: block;}
.order-sapce-wrapper{overflow: hidden;background-color: #eef6fb;padding: 25px 70px;-webkit-box-shadow: 3px 4px 13px -8px;-moz-box-shadow: 3px 4px 13px -8px;box-shadow: 2px 4px 37px -7px;position: relative;}
.order-space-rows{float: left;padding: 5px 10px;width: 170px;margin-right: 10px;margin-bottom: 20px;border: 1px solid #333333;}
.order-space-portrait{float: left;width: 60px;border-radius: 50%;}
.order-space-name{padding: 23px 0;margin-left: 15px;float: left;}
.client-more-active{height: 47px;overflow: hidden;}
.client-more-active .client-more-ico{ background: url('../images/fund/xiala_ico_2.png') no-repeat;}
.client-small-active{height: 65px;overflow: hidden;}
.client-small-active .order-setting-xiala{background: url('../images/ico_xiala02.png') no-repeat;}
.order-number-sroll{position: relative;float: right;height: 34px;width: 26px;}
.order-number-top{width: 16px;height: 16px;background: url('../images/ico_xiala02.png') no-repeat;margin: 6px 7px;cursor: pointer;position: absolute;left: -7px;top: 0px;}
.order-number-bottom{width: 16px;height: 16px;background: url('../images/ico_xiala.png') no-repeat;margin: 6px 7px;cursor: pointer;position: absolute;left: -6px;top: 8px;}