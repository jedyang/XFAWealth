.tasklist-metting-choice:after{content: "";display: block;border-color: transparent;border-bottom-color: #ffffff;border-width: 8px;border-style: solid;width: 0;height: 0;position: absolute;top: -14px;left: 85px;}
.line-add-label{position: absolute;padding-bottom: 40px;background-color: white;width: 100px;left: 52%;top: 40px;border: 1px solid #e4e4e4;box-shadow: 0px 0px 2px #ccc7c7;padding-left: 20px;}
